{
  "name": "thailpha",
  "version": "3.1.2",
  "description": "Thai alphabet webapp",
  "author": "Yannick AÃ¯vayan <yannick.aivayan@hiswe.net> (http://hiswe.net)",
  "main": "dist/index.html",
  "repository": "git://github.com/Hiswe/thailpha",
  "keywords": [
    "thai",
    "alphabet"
  ],
  "engines": {
    "node": "8.10.0"
  },
  "scripts": {
    "test": "gulp data && ava",
    "start": "npm run build:prod && npm run serve:prod",
    "dev": "gulp dev",
    "doctoc": "doctoc README.md --github",
    "build": "gulp build:prod --compress",
    "build:prod": "yarn run build",
    "build:dev": "gulp build",
    "build:js": "gulp js",
    "build:js:workbox": "gulp js:workbox",
    "serve:prod": "ws --https --port 5000 --directory dist --spa index.html",
    "serve:dev": "ws --https --port 5000 --directory dist --spa index.html --log.format tiny",
    "serve:dev-no-https": "ws --port 4000 --directory dist --spa index.html --log.format tiny",
    "watch:prod": "npm run build:prod --compress && gulp watch --compress",
    "watch:dev": "npm run build:dev && gulp watch",
    "pre-release": "gulp pre-release --compress --release",
    "pre-release:no:bump": "gulp pre-release --compress --release --no-bump",
    "deploy": "node ./bin/release.js",
    "release:no-bump": "npm run pre-release:no-bump && node ./bin/release.js --no-bump",
    "release": "npm run test && npm run pre-release && npm run deploy",
    "pre-release:gh": "npm run pre-release -- --dest=gh",
    "pre-release:gh:no-bump": "npm run pre-release:no-bump -- --dest=gh",
    "deploy:gh": "npm run deploy -- --dest=gh",
    "release:gh:no-bump": "npm run pre-release:gh:no-bump && npm run deploy:gh -- --no-bump"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "babel": false,
    "compileEnhancements": false,
    "require": [
      "@babel/register"
    ]
  },
  "license": "MIT",
  "dependencies": {
    "crio": "^5.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.0",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/register": "^7.0.0-beta.42",
    "@babel/runtime": "^7.0.0-beta.42",
    "ansi-magenta": "^0.1.1",
    "autoprefixer": "^8.2.0",
    "ava": "^1.0.0-beta.3",
    "babel-loader": "^8.0.0-beta.2",
    "beeper": "^1.1.1",
    "browser-sync": "^2.23.6",
    "connect-history-api-fallback": "^1.3.0",
    "connect-logger": "^0.0.1",
    "csswring": "^6.0.1",
    "del": "^3.0.0",
    "doctoc": "^1.3.0",
    "fancy-log": "^1.3.2",
    "firebase-tools": "^3.17.4",
    "fs-extra": "^5.0.0",
    "gulp": "^4.0.0",
    "gulp-cheerio": "^0.6.2",
    "gulp-cheerio-clean-svg": "git+https://github.com/Hiswe/gulp-cheerio-clean-svg",
    "gulp-define-module": "^0.1.1",
    "gulp-gzip": "^1.4.2",
    "gulp-if": "^2.0.2",
    "gulp-image-resize": "^0.13.0",
    "gulp-json-editor": "^2.2.1",
    "gulp-jsoncombine": "^1.0.2",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^7.0.0",
    "gulp-pug": "^3.3.0",
    "gulp-purge-sourcemaps": "^1.0.0",
    "gulp-rename": "^1.2.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-stylus": "^2.0.0",
    "gulp-svg-symbols": "^3.0.0",
    "inquirer": "^5.0.1",
    "lazypipe": "^1.0.1",
    "local-web-server": "^2.4.0",
    "merge-stream": "^1.0.1",
    "shelljs": "^0.8.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^4.2.0",
    "workbox-build": "^3.0.1",
    "yargs": "^11.0.0"
  }
}
